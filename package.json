{
  "name": "expenses-tracker-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "dev": "nodemon -r tsconfig-paths/register src/index.ts",
    "start": "node dist/index.js",
    "format": "prettier --write .",
    "prepare": "husky",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx pretty-quick --staged && npm run check-lint && npm run check-format && npm run check-types"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "moment": "^2.30.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.4",
    "concurrently": "^9.1.2",
    "eslint": "^9.20.1",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.1",
    "pretty-quick": "^4.0.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  }
}